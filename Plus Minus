y=input()
x=input()
x=x.split()
zero=0
pos=0
neg=0
a=0
for j in x:
    j=int(j)
    x[a]=j
    a=a+1
for i in range(len(x)):
    if x[i] == 0:
        zero+=1
    elif x[i] > 0:
        pos+=1
    elif x[i] < 0:
        neg+=1
y=int(y)
res1= pos/y
res2=neg/y
res3=zero/y
print(res1)
print(res2)
print(res3)
